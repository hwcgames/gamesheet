	"version": "0.2.0",
	"configurations": [
		{
			"type": "lldb",
			"request": "launch",
			"name": "Debug unit tests in library 'gamesheet_core'",
			"cargo": {
				"args": [
					"test",
					"--no-run",
					"--lib",
					"--package=gamesheet_core"
				],
				"filter": {
					"name": "gamesheet_core",
					"kind": "lib"
				}
			},
			"args": [],
			"cwd": "${workspaceFolder}"
		},
		{
			"type": "lldb",
			"request": "launch",
